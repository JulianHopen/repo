<section>
  <br />
  <br />
  <div class="container">
  <br />
  <h1 class="title has-text-centered">Active ticket overview</h1>
<br><br>
  <div class="columns">
    <% display.forEach(function(result){ %>
    <div class="column"><div class="box">
      <a href="mailto:<%= result.email %>">
        <p> <%= result.email %>
        <br><br>
            <%= result.text %></p></div></a>
            <form method="post" action="/support-admin/<%= result.id %>">
              <button class="button" type="submit">Archive</button>
            </form>
      <hr />
    </div>
  
    <% }) %>
  </div>
</section>
